lname="manny-layer"
profile="training"
bucket="em-employeedata"

aws lambda publish-layer-version \
    --profile ${profile} \
    --layer-name "${lname}" \
    --description "Python layer" \
    --license-info "MIT" \
    --content S3Bucket="${bucket}",S3Key="lambda/layers/python/aim_edl_layer_v1/aim_edl_layer_v1.zip " \
    --compatible-runtimes python3.6 python3.7 python3.8
exit 0
aws lambda add-layer-version-permission \
   --profile ${profile} \
   --layer-name ${lname} \
   --statement-id "xaccount" \
   --action lambda:GetLayerVersion  \
   --principal ${accountid} \
   --version-number 1 \
   --output text
exit 0